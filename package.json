{
  "name": "@fergalhanley/car-catalogue",
  "version": "1.0.0",
  "description": "Quantas tech challenge app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "npm run lint && node tools/prod-build.js",
    "start": "node index.js",
    "lint": "eslint **/*.js?",
    "dev": "nodemon index.js dev"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "classes": true,
      "jsx": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/display-name": 1
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "lib"
    ]
  },
  "author": "Fergal Hanley <fergalhanley@gmail.com> (http://fergalhanley.com)",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "express": "^4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.0",
    "webpack-dev-server": "^2.4.1"
  }
}
